{
	"name": "express-basics",
	"version": "0.0.1",
	"description": "This is a server built using Express",
	"main": "app.js",
	"scripts": {
		"sayHello": "echo \"Hello\"",
		"start": "nodemon app.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"db:create": "createdb --echo knex_db",
		"db:drop": "dropdb --if-exists --echo knex_db",
		"db:migrate": "knex migrate:latest",
		"db:reset": "npm run db:drop && npm run db:create && npm run db:migrate"
	},
	"keywords": [
		"Express",
		"Http",
		"Server"
	],

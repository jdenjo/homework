	"dependencies": {
		"cookie-parser": "^1.4.3",
		"ejs": "^2.6.1",
		"express": "^4.16.4",
		"knex": "^0.16.3",
		"method-override": "^3.0.0",
		"morgan": "^1.9.1",
		"pg": "^7.8.0"
	},
	"devDependencies": {
		"faker": "^4.1.0",
		"nodemon": "^1.18.9"
	}
}
